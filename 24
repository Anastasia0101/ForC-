
#include <iostream>
#include <string>
#include <math.h>

using namespace std;

int main() {
    int x1, y1, x2, y2;
    float S;
    
    cout << "Input x1: ";
    cin >> x1;
    cout << "Input y1: ";
    cin >> y1;
    cout << "Input x2: ";
    cin >> x2;
    cout << "Input y2: ";
    cin >> y2;
    
    S = hypot (x1 - x2,y1 - y2);
    
    cout << "S = " << S << endl;
   
  return 0; 
}
