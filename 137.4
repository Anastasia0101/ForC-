#include <iostream>
#include <string>
#include <cmath>

using namespace std;

int main()
{
    int n,j;
    float mul;

    cout << "Input n: " ;
    cin >> n;
    
    j = 1;
    mul = 1;
    for (int i = 1; i < n + 1; i++){
        mul *= i * j * pow((-1),i + 1);
        cout << "Result: " << mul << endl; 
        }
}
