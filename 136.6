#include <iostream>
#include <string>
#include <cmath>

using namespace std;

int main()
{
    int n;
    int sum, mul;

    cout << "Input n: " ;
    cin >> n;

    sum = 0;
    mul = 1;
    for (int i = 1; i < n + 1; i++){
        sum += i;
        mul *= i;
        }
    cout << "Sum: " << sum << endl; 
    cout << "Mul: " << mul << endl; 
}
