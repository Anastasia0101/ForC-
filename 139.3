#include <iostream>
#include <cmath>

using namespace std;
 
int fact(int n) {
    if ((n==0)||(n==1))
        return 1;
    else 
        return n*fact(n-1);
}

int main()
{
    int n;
    float b;
    
    cout << "Input n: ";
    cin >> n;
    
    for ( int i = 1; i < n + 1; i++ ){
        b = fact(i);
    cout << b << endl;
}
    return 0;
}
